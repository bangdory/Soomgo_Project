        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.soomgo.soomgo_project.mappers.user.ExpertMapper">

    <select id="findByID" resultType="org.soomgo.soomgo_project.domain.expert.ExpertDTO">
